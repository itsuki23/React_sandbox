{
  "name": "canvash",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Test for Hakkason GW in 2020",
  "icons": {
    "16": "images/icon/top-logo-hand.png"
  },

  "browser_action":{
    "default_icon":{
        "16": "images/icon/top-logo-hand.png"
    },
    "default_title": "インスタントメモ帳",
    "default_popup": "index.html"
  },

  "commands": {
    "_execute_browser_action": {
       "suggested_key": {
          "chromeos": "Alt+Q",
          "default": "Alt+Q",
          "mac": "Alt+Q",
          "windows": "Alt+Q"
       }
    }
 },

  "permissions": [
    "storage"
  ]
}